  "nz_popover_help-html": {
    "prefix": "nz-popover-help-html",
    "body": "<nz-popover [nzContent]=\"${1:help}Tpl\">\n  <ng-template #${1}Tpl>\n    $0\n  </ng-template>\n  <i nz-popover nz-icon type=\"question-circle\"></i>\n</nz-popover>",
    "description": "Help demo (Include html)"
  },
  "nz_popover_help": {
    "prefix": "nz-popover-help",
    "body": "<nz-popover nzContent=\"$0\">\n  <i nz-popover nz-icon type=\"question-circle\"></i>\n</nz-popover>",
    "description": "Help demo"
  },
  "nz_radio_button": {
    "prefix": "nz-radio-button",
    "body": "<nz-radio-group [(ngModel)]=\"$1\">\n  <label nz-radio-button nzValue=\"A\">A</label>\n  <label *ngFor=\"let ${3:item} of ${2:list}\" nz-radio-button [nzValue]=\"$3\">{{${3}.text}}</label>\n</nz-radio-group>\n$0",
    "description": "Radio button style"
  },
  "nz_radio": {
    "prefix": "nz-radio",
    "body": "<nz-radio-group [(ngModel)]=\"$1\">\n  <label nz-radio nzValue=\"A\">A</label>\n  <label *ngFor=\"let ${3:item} of ${2:list}\" nz-radio [nzValue]=\"$3\">{{${3}.text}}</label>\n</nz-radio-group>\n$0",
    "description": "Radio"
  },
  "nz_radio_simple": {
    "prefix": "nz-radio-simple",
    "body": "<label nz-radio [nzValue]=\"${1}\">$0</label>",
    "description": "Radio"
  },
  "nz_select": {
    "prefix": "nz-select",
    "body": "<nz-select [(ngModel)]=\"${1:value}\" name=\"${1}\" nzPlaceHolder=\"${2}\"${3: nzAllowClear}>\n  <nz-option\n    *ngFor=\"let ${5:item} of ${4:list}\"\n    [nzLabel]=\"$5.label\"\n    [nzValue]=\"$5.value\"\n    [nzDisabled]=\"$5.disabled\">\n  </nz-option>\n</nz-select>\n$0",
    "description": "Select"
  },
  "nz_select_option-group": {
    "prefix": "nz-select-option-group",
    "body": "<nz-option-group [nzLabel]=\"${1:item}\"></nz-option-group>",
    "description": "Select option group"
  },
  "nz_select_option": {
    "prefix": "nz-select-option",
    "body": "<nz-option\n  *ngFor=\"let ${5:item} of ${4:list}\"\n  [nzLabel]=\"$5.label\"\n  [nzValue]=\"$5.value\"\n  [nzDisabled]=\"$5.disabled\"\n  [nzCustomContent]=\"tpl\">\n  <ng-template #tpl>\n    {{ $5 }}\n  </ng-template>\n</nz-option>",
    "description": "Select option"
  },
  "nz_spin": {
    "prefix": "nz-spin",
    "body": "<nz-spin${1: [nzTip]=\"'${2|Loading...|}'\"}${3: [nzSpinning]=\"false\"}></nz-spin>\n$0",
    "description": "Spin"
  },
  "nz_steps_custom-dot": {
    "prefix": "nz-steps-custom-dot",
    "body": "<nz-steps [(nzCurrent)]=\"${1:current}\" [nzProgressDot]=\"${2:progressTemplate}\">\n  <nz-step nzTitle=\"Finished\" nzDescription=\"You can hover on the dot.\"></nz-step>\n  $0\n</nz-steps>\n<ng-template #${2} let-dot let-status=\"status\" let-index=\"index\">\n</ng-template>",
    "description": "Custom dot"
  },
  "nz_steps": {
    "prefix": "nz-steps",
    "body": "<nz-steps [(nzCurrent)]=\"${1:current}\"${2: [nzStatus]=\"'${3|wait,process,finish,error|}'\"}${4: nzSize=\"small\"}${5: nzDirection=\"${6|horizontal,vertical|}\"}${7: nzProgressDot}>\n  <nz-step [nzTitle]=\"'${8}'\" [nzDescription]=\"'${9}'\"></nz-step>\n  $0\n</nz-steps>\n<div class=\"steps-content\">\n\n</div>",
    "description": "Steps"
  },
  "nz_steps_step-icon": {
    "prefix": "nz-steps-step-icon",
    "body": "<nz-step [nzIcon]=\"nzIconTpl\" [nzTitle]=\"'${1}'\" [nzDescription]=\"'${2}'\"${3: [nzStatus]=\"'${4|wait,process,finish,error|}'\"}>\n  <ng-template #nzIconTpl>$0</ng-template>\n</nz-step>",
    "description": "Steps inlcudes step and icon"
  },
  "nz_steps_step": {
    "prefix": "nz-steps-step",
    "body": "<nz-step [nzTitle]=\"'${1}'\" [nzDescription]=\"'${2}'\"${3: [nzStatus]=\"'${4|wait,process,finish,error|}'\"}></nz-step>",
    "description": "Step in stepds"
  },
  "nz_switch": {
    "prefix": "nz-switch",
    "body": "<nz-switch [(ngModel)]=\"${1}\" name=\"${1}\"></nz-switch>\n$0",
    "description": "Switch"
  },
  "nz_switch_icon": {
    "prefix": "nz-switch-icon",
    "body": "<nz-switch [(ngModel)]=\"${1}\" name=\"${1}\" [nzCheckedChildren]=\"checkedTemplate\" [nzUnCheckedChildren]=\"unCheckedTemplate\">\n  <ng-template #checkedTemplate><i nz-icon type=\"${2:check}\"></i></ng-template>\n  <ng-template #unCheckedTemplate><i nz-icon type=\"${3:close}\"></i></ng-template>\n</nz-switch>\n$0",
    "description": "Switch includes icon"
  },
  "nz_switch_text": {
    "prefix": "nz-switch-text",
    "body": "<nz-switch [(ngModel)]=\"${1}\" name=\"${1}\" nzCheckedChildren=\"${2|Opened,Enabled|}\" nzUnCheckedChildren=\"${3|Closed,Disabled|}\"></nz-switch>\n$0",
    "description": "Switch includes text"
  },
  "nz_table": {
    "prefix": "nz-table",
    "body": "<nz-table #nzTable [nzData]=\"${1}\" [nzPageSize]=\"${2:10}\">\n  <thead>\n    <tr>\n      <th>Title</th>\n      $3\n      <th></th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr *ngFor=\"let i of nzTable.data\">\n      <td>{{i.title}}</td>\n      $4\n      <td>\n        <a href=\"#\" (click)=\"edit(i)\">Edit</a>\n        <nz-divider nzType=\"vertical\"></nz-divider>\n        <a href=\"#\" (click)=\"del(i)\">Remove</a>${5:\n        <nz-divider nzType=\"vertical\"></nz-divider>\n        <nz-dropdown>\n          <a class=\"ant-dropdown-link\" nz-dropdown>\n            More <i nz-icon type=\"${6|down,ellipsis|}\"></i>\n          </a>\n          <ul nz-menu>\n            <li nz-menu-item>\n              <a (click)=\"\">active</a>\n            </li>\n          </ul>\n        </nz-dropdown>\n        }\n      </td>\n    </tr>\n  </tbody>\n</nz-table>",
    "description": "Table"
  },
  "nz_table_static-no-pager": {
    "prefix": "nz-table-static-no-pager",
    "body": "<nz-table [nzData]=\"${1|list,data|}\" [nzShowPagination]=\"false\">\n  <thead>\n    <tr>\n      <th>$0</th>\n      <th nzWidth=\"300px\"></th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr *ngFor=\"let ${2:i} of $1\">\n      <td>{{${2}.title}}</td>\n      <td>\n        <a (click)=\"next(${2})\">Next</a>\n        <nz-divider nzType=\"vertical\"></nz-divider>\n        <a (click)=\"edit(${2})\">Edit</a>\n        <nz-divider nzType=\"vertical\"></nz-divider>\n        <nz-popconfirm [nzTitle]=\"'Are you sure?'\" (nzOnConfirm)=\"del(${2})\">\n          <a nz-popconfirm>Delete</a>\n        </nz-popconfirm>\n      </td>\n    </tr>\n  </tbody>\n</nz-table>",
    "description": "Table not pagination"
  },
  "nz_tabs": {
    "prefix": "nz-tabs",
    "body": "<nz-tabset>\n  <nz-tab nzTitle=\"Tab 1\">\n    Content of Tab Pane 1\n  </nz-tab>\n  <nz-tab nzTitle=\"Tab 2\">\n    Content of Tab Pane 2\n  </nz-tab>\n  <nz-tab nzTitle=\"Tab 3\">\n    Content of Tab Pane 3\n  </nz-tab>\n</nz-tabset>",
    "description": "Tabs"
  },
  "nz_tag": {
    "prefix": "nz-tag",
    "body": "<nz-tag nzColor=\"${2|magenta,red,volcano,orange,gold,lime,green,cyan,blue,geekblue,purple|}\">$1</nz-tag>",
    "description": "Tag"
  },
  "nz_textarea": {
    "prefix": "nz-textarea",
    "body": "<textarea nz-input [(ngModel)]=\"${1:remark}\" id=\"${1}\" name=\"${1}\" [nzAutosize]=\"${2|true,false,{ minRows: 2 <Change to comma by yourself> maxRows: 6 }|}\"${3: placeholder=\"${4}\"}></textarea>$0",
    "description": "Textarea"
  },
  "nz_time": {
    "prefix": "nz-time",
    "body": "<nz-time-picker [(ngModel)]=\"${1:value}\" name=\"${1}\" nzPlaceHolder=\"${3|Please select time|}\"></nz-time-picker>",
    "description": "Time picker"
  },
  "nz_tooltip": {
    "prefix": "nz-tooltip",
    "body": "<a nz-tooltip=\"$2\">$1</a>$0",
    "description": "Tooltip"
  },
  "nz_tooltip_template": {
    "prefix": "nz-tooltip-template",
    "body": "<nz-tooltip>\n  <span nz-tooltip>${1}</span>\n  <ng-template #nzTemplate>\n    <i nz-icon type=\"file\"></i>\n    <span>$0</span>\n  </ng-template>\n</nz-tooltip>",
    "description": "Tooltip curstom template"
  },
  "nz_upload": {
    "prefix": "nz-upload",
    "body": "<nz-upload nzAction=\"${1:/upload}\">\n  <button nz-button>\n    <i nz-icon type=\"upload\"></i>\n    <span>${2:Upload}</span>\n  </button>\n</nz-upload>\n$0",
    "description": "Upload"
  },
  "nz_upload_drag": {
    "prefix": "nz-upload-drag",
    "body": "<nz-upload nzType=\"drag\"\n  nzAction=\"${1:/upload}\"\n  (nzChange)=\"handleChange($event)\"\n  [nzMultiple]=\"true\"\n  [nzLimit]=\"${2:5}\">\n  <p class=\"ant-upload-drag-icon\">\n    <i nz-icon type=\"inbox\"></i>\n  </p>\n  <p class=\"ant-upload-text\">Click or drag file to this area to upload</p>\n  <p class=\"ant-upload-hint\">Support for a single or bulk upload. Strictly prohibit from uploading company data or other band files</p>\n</nz-upload>\n$0",
    "description": "Upload of drag"
  }
}
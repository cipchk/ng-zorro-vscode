{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "g",
      "program": "${workspaceFolder}/scripts/build.js",
      "args": [
        "button"
      ]
    },
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "generate",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": [
        "prod"
      ]
    }
  ]
}